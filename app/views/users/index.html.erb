<%= render "layouts/header" %>
<div class="container">
  <div class="row">
    <div class="col-md-6">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
          <tr>
              <td>
                <%= attachment_image_tag user, :profile_image, fallback: "no-image.jpg", class: "profile_image" %>
              </td>
              <td>
                <p><%= user.name%></p>
                <p><%= user.profile %></p>
              </td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>